{
    "name": "blesta/composer-installer",
    "type": "composer-plugin",
    "require": {
        "composer/installers": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Blesta\\Composer\\Installer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Blesta\\Composer\\Installer\\Tests": "tests/"
        }
    },
    "extra": {
        "class": "Blesta\\Composer\\Installer\\Installer"
    }
}
